package com.assignments;

import java.util.*;

public class SumOfCubes {

	static int CubeSum(int n) {
		int sum = 0;

		for (int i = 1; i <= n; i++)
			sum = sum + i * i * i;
		return sum;
	}

	public static void main(String[] args) {

		System.out.println("enter a number");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		System.out.println(CubeSum(n));

	}

}
